{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=DocumentProcessorDB;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "DocumentStorage": {
    "Provider": "LocalFileSystem",
    "LocalFileSystem": {
      "RootPath": "C:\\DocumentStorage",
      "MaxFileSizeInMB": 100,
      "AllowedExtensions": [".pdf", ".doc", ".docx", ".txt", ".rtf", ".odt"]
    },
    "S3": {
      "BucketName": "document-processor-bucket",
      "Region": "us-east-1",
      "UsePresignedUrls": true,
      "PresignedUrlExpirationMinutes": 60
    },
    "FileShare": {
      "NetworkPath": "\\\\fileserver\\documents",
      "Username": "",
      "Domain": "",
      "MaxFileSizeInMB": 100
    }
  },
  "AIProcessing": {
    "DefaultProvider": "Mock",
    "MaxRetries": 3,
    "RetryDelaySeconds": 5,
    "Providers": {
      "Mock": {
        "Enabled": true
      },
      "AmazonBedrock": {
        "Enabled": true,
        "Region": "us-west-2",
        "ModelId": "anthropic.claude-3-sonnet-20240229-v1:0",
        "MaxTokens": 4096,
        "Temperature": 0.7
      }
    }
  },
  "Bedrock": {
    "Region": "us-west-2",
    "ClassificationModelId": "anthropic.claude-3-haiku-20240307-v1:0",
    "ExtractionModelId": "anthropic.claude-3-sonnet-20240229-v1:0",
    "SummarizationModelId": "anthropic.claude-3-haiku-20240307-v1:0",
    "IntentModelId": "anthropic.claude-3-haiku-20240307-v1:0",
    "MaxTokens": 2000,
    "Temperature": 0.3,
    "TopP": 0.9,
    "MaxRetries": 3,
    "RetryDelayMilliseconds": 1000,
    "EnableDetailedLogging": false,
    "UseMockResponses": false,
    "AwsProfile": null
  }
}